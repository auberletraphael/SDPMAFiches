      procédure AfficheImages()
      Sablier(Vrai)
      
      JAUGE_Jauge.Valeur = 0
      
      
      ZoneRépétéeSupprimeTout(ZR_images)
      
      tabLeauImages est un tableau de variants = PixaBayCherche(SAI_recherche)
      
      JAUGE_Jauge.BorneMax = tabLeauImages..Occurrence
      
      si tabLeauImages..Occurrence = 0 ALORS
      	sLienrecherche est une chaîne = "https://www.google.com/search?q="
      	sChaineRecherche est une chaine = Remplace(SAI_recherche," ","+")
      	
      	sLienrecherche += sChaineRecherche
      	
      	
      	si OuiNon("Aucune Image trouvée sur pixabay, voulez vous lancer une recherche Google ?") ALORS
      		LanceAppliAssociée(sLienrecherche)
      		Ferme(FEN_Recherche_d_Image)
      		
      		
      	FIN
      	
      	
      FIN
      
      pour i=1 _A_ tabLeauImages..Occurrence
      	vAriantRetourimage est un variant = tabLeauImages[i]
      	
      //	vAriantRetourimage.ImagePrev
      //	vAriantRetourimage.ImageWeb 
      	
      	ZoneRépétéeAjoute(ZR_images,vAriantRetourimage.ImagePrev + tab + vAriantRetourimage.ImageWeb)
      	
      	JAUGE_Jauge.Valeur = jAUGE_Jauge.Valeur + 1
      		
      FIN
      
      Sablier(fauxToutSablier)

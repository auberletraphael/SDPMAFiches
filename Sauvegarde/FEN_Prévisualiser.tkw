        type : 14
      -
        code : |1-
         si gsCheminsauve <> "" ALORS
         	
         	// Initialise le générateur de nombres aléatoires
         	InitHasard()
         	
         	nNombreAléatoire est un entier
         	nNombreAléatoire = Hasard(0, 9000000)
         	
         	
         	fRepCrée(gsCheminsauve + "Images")
         	
         	si dSauveImageJPEG(gImageBrute,gsCheminsauve + "\Images\" + nNombreAléatoire + ".jpg",70) ALORS
         		gsNomImage = nNombreAléatoire + ".jpg"
         		sauvegardeImageMin(nNombreAléatoire)
         		Ferme(FEN_Prévisualiser)
         	FIN
         FIN
         
         // test sauvegarde miniature pour excel
         
         procédure interne sauvegardeImageMin(nBaléatoire est un entier)
         	monImage est une Image
         	monImage = gImageBrute
         	dRedimensionne(monImage,150,110,drHomothétiqueCentré)
         	
         	dSauveImageJPEG(monImage,gsCheminsauve + "\Images\" + nBaléatoire + "_min.jpg",70)
         	
         FIN

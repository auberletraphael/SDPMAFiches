      	gnTypeFiche = 1
      	FEN_Principale..Libellé = gsLibFenetrePrincipale + " FICHE VEGETAUX"
      sinon
      	//ANI
      	gnTypeFiche = 2
      	FEN_Principale..Libellé = gsLibFenetrePrincipale + " FICHE ANNIMALERIE"
      FIN
